## Key bindings for ncmpc (generated by ncmpc)

## Key configuration screen
key screen-keyedit = '0',  F10,  Ctrl-K

## Quit
key quit = 'q',  'Q'

## Move cursor up
key up = 259,  'k'

## Move cursor down
key down = 258,  'j'

## Move cursor to the bottom of screen
key bottom = 'L'

## Move cursor to the top of the list
key home = 262,  'g'

## Move cursor to the bottom of the list
key end = 360,  'G'

## Page up
key pgup = 339,  Ctrl-B,  'b'

## Page down
key pgdn = 338,  Ctrl-F,  'f'

## Scroll down one line
key scroll-down-line = Ctrl-E

## Scroll up one line
key scroll-up-line = Ctrl-Y

## Scroll up half a screen
key scroll-down-half = Ctrl-D

## Scroll down half a screen
key scroll-up-half = Ctrl-U

## Select currently playing song
key select-playing = 263

## Help screen
key screen-help = '1',  F1

## Play/Enter directory
key play = Ctrl-M,  'l'

## Pause
key pause = 'p',  32

## Stop
key stop = 's'

## Seek forward
key seek-forward = 261

## Seek backward
key seek-backward = 260

## Increase volume
key volume-up = 43,  61

## Decrease volume
key volume-down = 45

## Select/deselect song in queue
key select = 't'

## Select all listed items
key select_all = 'T'

## Clear queue
key clear = 'C'

## Toggle consume mode
key consume = 'c'

## Start a music database update
key db-update = 'U'

## Go to parent directory
key go-parent-directory = 'h'

## Locate song in browser
key locate = Ctrl-G

## Move item up
key move-up = 'K'

## Move item down
key move-down = 'J'

## Toggle auto center mode
key autocenter-mode = 0

## Next screen
key screen-next = Ctrl-I,  46

## Previous screen
key screen-prev = 353,  44

## Backward find
key rfind = 63

## Backward find previous
key rfind-next = 'N'

## Jump to
key jump = Ctrl-J

## View the selected and the currently playing song
key view = '9',  F9

## Lyrics screen
key screen-lyrics = '6',  F6

## Update Lyrics
key lyrics-update = 0

## Outputs screen
key screen-outputs = '7',  F7

## Chat screen
key screen-chat = '8',  F8

